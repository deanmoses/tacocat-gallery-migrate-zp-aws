    "description": "Scripts to migrate Tacocat Gallery from Zenphoto to AWS",
    "author": "Dean Moses",
    "license": "ISC",
    "type": "module",
    "engineStrict": true,
    "engines": {
        "node": ">=20.0.0"
    },
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "zenphoto": "node --loader ts-node/esm src/index.js",
        "verify-local-files": "node --loader ts-node/esm src/verifyLocalFiles.js",
        "verify-local-db": "node --loader ts-node/esm src/verifyLocalDb.js",
