        "@typescript-eslint/parser": "^6.13.2",
        "eslint": "^8.55.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.0.1",
        "jest": "^29.7.0",
        "prettier": "^3.1.1",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@aws-sdk/client-dynamodb": "^3.470.0",
        "@aws-sdk/client-s3": "^3.472.0",
